
@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="section no-pad-bot" id="index-banner" style="background-image: url(../../Imagens/pizza.jpg)">
    <div class="container" style="z-index: 9999">
        <br />
        <fieldset class="row">
            <h1 class="text_h center header cd-headline letters type">
                <span class="cd-words-wrapper waiting">
                    <b class="is-visible">Busque</b>
                    <b>Peça</b>
                </span>
                <span>AGORA</span>
            </h1>

            <div class="col-md-3">
            </div>
            <div class="col-md-6 pesquisaHome">
                <form class="form floating-label" action="@Url.Action("Index", "Pesquisar")">
                    <div class="col-md-12">
                        <div class="form-group">
                            <select id="ddlTipo" name="ddlTipo" class="white-text form-control">
                                <option value=""></option>
                                <option value="1" class="white-text">Hamburguer</option>
                                <option value="2" class="white-text">Pizza</option>
                                <option value="3" class="white-text">Refeição</option>
                            </select>
                            <label for="ddlTipo" class="white-text">Quero comer</label>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <select id="ddlOnde" class="white-text form-control">
                                <option></option>
                                <option class="white-text">Guapimirim</option>
                                <option class="white-text">Teresopólis</option>
                                <option class="white-text">Magé</option>
                            </select>
                            <label for="ddlOnde" class="white-text">Em</label>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group text-center">
                            <button class="btn waves-effect waves-light red darken-1" type="submit">
                                Pesquisar
                                <i class="mdi-content-send right white-text"></i>
                            </button>
                        </div>
                    </div>
                </form>
            </div>
            <div class="col-md-3">
            </div>
        </fieldset>
    </div>
</div>
